<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<?import io.github.palexdev.materialfx.controls.MFXButton?>
<StackPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.dkkp.controller.ImportDetailController"
           fx:id="main" styleClass="productAP">
  <VBox spacing="10">
    <MFXButton fx:id="bckBtn" buttonType="RAISED" mnemonicParsing="false" text="Back" prefWidth="70" styleClass="Btn"/>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="ID BASE PRODUCT:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="NAME PRODUCT:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="TOTAL QUANTITY:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="DATE RELEASE:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="DESCRIPTION:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="VIEW COUNT:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="ID CATEGORY:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
    <HBox spacing="5">
      <VBox.margin>
        <Insets left="90"/>
      </VBox.margin>
      <Label text="ID BRAND:" styleClass="pdLabel"/>
      <Label text="0"/>
    </HBox>
  </VBox>
</StackPane>