<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<VBox alignment="CENTER" spacing="10" maxWidth="450" styleClass="loginVB">
  <GridPane alignment="CENTER" hgap="10" vgap="10">
    <columnConstraints>
      <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" prefWidth="150"/>
    </columnConstraints>
    <MFXTextField floatingText="ID Brand" floatMode="BORDER" prefWidth="150" GridPane.columnIndex="0"
                  GridPane.rowIndex="0"/>
    <MFXTextField floatingText="Name Brand" floatMode="BORDER" prefWidth="150" GridPane.columnIndex="0"
                  GridPane.rowIndex="1"/>
    <rowConstraints>
      <RowConstraints/>
      <RowConstraints/>
    </rowConstraints>
  </GridPane>
  <HBox alignment="CENTER">
    <MFXButton buttonType="RAISED" mnemonicParsing="false" text="Submit" styleClass="Btn"/>
  </HBox>
</VBox>